<div class='wrapper'>

  <%= render 'form' %>

  <% @articles.each do |article| %>
    <li>
      ><strong><%= article.title %></strong>
       <%= link_to "show", article_path(article) %>
       <%= link_to "edit", edit_article_path(article) %>
       <%= link_to "delete", article_path(article), method: :delete, data: { confirm: "Are you sure?" } %>
    </li>
  <% end %>
</div>
